(define x 48)

(define (incr) (set! x (+ x 1)) x)

(define (read-file file)
  (let ((port (open-input-file file)))
    (let loop((ls '()) (char (read-char port)))
      (cond ((eof-object? char)
	  (begin
	    (close-input-port port)
	    (list->string (reverse ls))))
            ((char=? char #\%)
	    (loop
             (cons #\space (cons #\- (cons #\space (cons (integer->char (incr)) ls))))
             (read-char port)))
	  (else (loop (cons char ls) (read-char port)))))))

(define (get-input input)
  (cond((number? input) (display "YES."))
       (else (display "NO."))))



